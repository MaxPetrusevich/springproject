<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                                       http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.15.xsd">

    <changeSet id="16" author="yourname">
        <createTable tableName="order_technique">
            <column name="order_technique_id" type="SERIAL">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="order_id" type="INTEGER"/>
            <column name="tech_id" type="INTEGER"/>
            <column name="quantity" type="INTEGER"/>
            <!-- Add other columns as needed -->
        </createTable>
        <addForeignKeyConstraint baseColumnNames="order_id"
                              baseTableName="Order"
                              constraintName="fk_order_technique_order"
                              referencedColumnNames="order_id"
                              referencedTableName="Order"/>
        <addForeignKeyConstraint baseColumnNames="tech_id"
                              baseTableName="Technique"
                              constraintName="fk_order_technique_technique"
                              referencedColumnNames="tech_id"
                              referencedTableName="Technique"/>

    </changeSet>
</databaseChangeLog>